<div id="header" class="span-24 last">
    <SCRIPT language="JavaScript">

    if (document.images)
    {
      preload_image_object = new Image();
      // set image url
      image_url = new Array();
      image_url[0] = "http://simplysushmita.com/images/home/designframe_color.png";
      image_url[1] = "http://simplysushmita.com/images/home/artframe_color.png";
      image_url[2] = "http://simplysushmita.com/images/home/resumeframe_color.png";
      image_url[3] = "http://simplysushmita.com/images/home/aboutframe_color.png";


       var i = 0;
       for(i=0; i<=3; i++) 
         preload_image_object.src = image_url[i];
    }

    //-->
    </SCRIPT>     
  


	<div id="logo" class="span-9">
	<% if @menuselect == "Home" %><%= link_to image_tag("logo3.png"), root_path, 
	:class => "selected" %>
	<% else %><%= link_to image_tag("logo4.png"), root_path %>
	<% end %>
	</div>
	<div id="nav" class="span-15 last" >
		<ul>
			<% if @menuselect == "Projects" %>
			<li><%= link_to "design", projects_path, :class => "selected" %></li>
			<% else %>
			<li><%= link_to "design", projects_path %></li>
			<% end %>

			<% if @menuselect == "Personal" %>
			<li><%= link_to "art", personals_path, :class => "selected" %></li>
			<% else %>
			<li><%= link_to "art", personals_path %></li>
			<% end %>
			
			<% if @menuselect == "Resume" %>
			<li><%= link_to "resume", resume_path, :class => "selected" %></li>
			<% else %>
			<li><%= link_to "resume", resume_path %></li>
			<% end %>
			
			<% if @menuselect == "About" %>
			<li><%= link_to "about", about_path, :class => "selected" %></li>
			<% else %>
			<li><%= link_to "about", about_path %></li>
			<% end %>
			
			
		</ul>
	</div>

</div>
